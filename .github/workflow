name:first

on: [push]

jobs:
    job1:
        name: First job
        runs-on: unbuntu-latest
        steps:
            -name: Step One
             uses: actions/checkout@v4
            -name: Step Tow  
             run : env /  sort
    job2:
        name: Second Job
        runs-on: windows-latest    
        steps: 
            -name: Step one
             uses:actions/checkout@v4
            -name: Step Tow 
            run: "Get-ChildItem Env: /Sort-Object Name"
